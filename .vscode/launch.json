    "version": "0.2.0",
    "configurations": [


        {
            "name": "Flutter Web (Chrome with Flags)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
              "-d",
              "Edge",
              
              "--web-browser-flag=--disable-web-security"
            ]
          },

        {
            "name": "piece_autos",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "piece_autos (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "piece_autos (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}